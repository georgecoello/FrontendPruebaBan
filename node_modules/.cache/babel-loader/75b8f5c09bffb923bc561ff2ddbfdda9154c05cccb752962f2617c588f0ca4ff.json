{"ast":null,"code":"import NavBar from '../components/Nav.vue';\nimport FooterPage from '../components/Footer.vue';\nimport ModalTransaccion from '../components/ModalTransaccion.vue'; // Importar el nuevo modal\nimport axios from 'axios';\nexport default {\n  name: 'TransaccionesPage',\n  components: {\n    NavBar,\n    FooterPage,\n    ModalTransaccion\n  },\n  data() {\n    return {\n      transacciones: [],\n      mostrarModalTransaccion: false // Controla la visibilidad del modal\n    };\n  },\n  async created() {\n    await this.cargarTransacciones();\n  },\n  methods: {\n    async cargarTransacciones() {\n      try {\n        const token = localStorage.getItem('token');\n        const response = await axios.get('http://127.0.0.1:8000/api/transacciones/', {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.transacciones = response.data;\n      } catch (error) {\n        console.error('Error al cargar transacciones:', error);\n      }\n    },\n    formatFecha(fecha) {\n      // Formatear la fecha para que sea más legible\n      return new Date(fecha).toLocaleDateString('es-ES', {\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    },\n    abrirModalCrearTransaccion() {\n      this.mostrarModalTransaccion = true; // Abrir el modal\n    },\n    async guardarTransaccion(nuevaTransaccion) {\n      try {\n        const token = localStorage.getItem('token');\n        await axios.post('http://127.0.0.1:8000/api/transacciones/', nuevaTransaccion, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        await this.cargarTransacciones(); // Recargar la lista de transacciones\n        this.mostrarModalTransaccion = false; // Cerrar el modal\n      } catch (error) {\n        console.error('Error al guardar la transacción:', error);\n        alert('Ocurrió un error al guardar la transacción.');\n      }\n    },\n    async eliminarTransaccion(id) {\n      if (confirm('¿Estás seguro de que deseas eliminar esta transacción?')) {\n        try {\n          const token = localStorage.getItem('token');\n          await axios.delete(`http://127.0.0.1:8000/api/transacciones/${id}/`, {\n            headers: {\n              Authorization: `Bearer ${token}`\n            }\n          });\n          await this.cargarTransacciones(); // Recargar la lista de transacciones\n        } catch (error) {\n          console.error('Error al eliminar la transacción:', error);\n          alert('Ocurrió un error al eliminar la transacción.');\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavBar","FooterPage","ModalTransaccion","axios","name","components","data","transacciones","mostrarModalTransaccion","created","cargarTransacciones","methods","token","localStorage","getItem","response","get","headers","Authorization","error","console","formatFecha","fecha","Date","toLocaleDateString","year","month","day","hour","minute","abrirModalCrearTransaccion","guardarTransaccion","nuevaTransaccion","post","alert","eliminarTransaccion","id","confirm","delete"],"sources":["/home/jorge/Escritorio/Jorge-PC/Prueba Bancooci/frontend/src/views/Transacciones.vue"],"sourcesContent":["<template>\n    <div class=\"min-h-screen flex flex-col bg-gray-100\">\n      <!-- NavBar -->\n      <NavBar />\n  \n      <!-- Contenido Principal (Ocupa todo el espacio disponible) -->\n      <main class=\"flex-grow container mx-auto p-4\">\n        <h1 class=\"text-3xl font-bold text-green-800 mb-4\">Transacciones</h1>\n        <div class=\"bg-white p-6 rounded-lg shadow-lg\">\n          <div class=\"flex justify-between items-center mb-4\">\n            <h2 class=\"text-xl font-bold text-green-800\">Lista de Transacciones</h2>\n            <button\n              @click=\"abrirModalCrearTransaccion\"\n              class=\"bg-green-600 text-white py-2 px-4 rounded-lg font-bold hover:bg-green-700 transition duration-300\"\n            >\n              Agregar Transacción\n            </button>\n          </div>\n  \n          <!-- Tabla de Transacciones -->\n          <table class=\"w-full\">\n            <thead>\n              <tr class=\"bg-green-100\">\n                <th class=\"px-4 py-2\">Fecha</th>\n                <th class=\"px-4 py-2\">Monto</th>\n                <th class=\"px-4 py-2\">Cliente</th>\n                <th class=\"px-4 py-2\">Agencia</th>\n                <th class=\"px-4 py-2\">Motivo</th>\n                <th class=\"px-4 py-2\">Acciones</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"transaccion in transacciones\" :key=\"transaccion.id\" class=\"border-b\">\n                <td class=\"px-4 py-2 text-center\">{{ formatFecha(transaccion.fechaTransaccion) }}</td>\n                <td class=\"px-4 py-2 text-center\">L. {{ transaccion.montoTransaccion }}</td>\n                <td class=\"px-4 py-2 text-center\">{{ transaccion.nombre_cliente }}</td>\n                <td class=\"px-4 py-2 text-center\">{{ transaccion.nombre_agencia }}</td>\n                <td class=\"px-4 py-2 text-center\">{{ transaccion.nombre_motivo_transaccion }}</td>\n                <td class=\"px-4 py-2 text-center\">\n                  <button\n                    @click=\"eliminarTransaccion(transaccion.id)\"\n                    class=\"bg-red-500 text-white py-1 px-2 rounded-lg hover:bg-red-600 transition duration-300\"\n                  >\n                    Eliminar\n                  </button>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </main>\n  \n      <!-- Footer (Siempre en la parte inferior) -->\n      <FooterPage />\n  \n      <!-- Modal para crear una nueva transacción -->\n      <ModalTransaccion\n        v-if=\"mostrarModalTransaccion\"\n        @close=\"mostrarModalTransaccion = false\"\n        @guardar=\"guardarTransaccion\"\n      />\n    </div>\n  </template>\n  \n  <script>\n  import NavBar from '../components/Nav.vue';\n  import FooterPage from '../components/Footer.vue';\n  import ModalTransaccion from '../components/ModalTransaccion.vue'; // Importar el nuevo modal\n  import axios from 'axios';\n  \n  export default {\n    name: 'TransaccionesPage',\n    components: {\n      NavBar,\n      FooterPage,\n      ModalTransaccion,\n    },\n    data() {\n      return {\n        transacciones: [],\n        mostrarModalTransaccion: false, // Controla la visibilidad del modal\n      };\n    },\n    async created() {\n      await this.cargarTransacciones();\n    },\n    methods: {\n      async cargarTransacciones() {\n        try {\n          const token = localStorage.getItem('token');\n          const response = await axios.get('http://127.0.0.1:8000/api/transacciones/', {\n            headers: { Authorization: `Bearer ${token}` },\n          });\n          this.transacciones = response.data;\n        } catch (error) {\n          console.error('Error al cargar transacciones:', error);\n        }\n      },\n      formatFecha(fecha) {\n        // Formatear la fecha para que sea más legible\n        return new Date(fecha).toLocaleDateString('es-ES', {\n          year: 'numeric',\n          month: 'long',\n          day: 'numeric',\n          hour: '2-digit',\n          minute: '2-digit',\n        });\n      },\n      abrirModalCrearTransaccion() {\n        this.mostrarModalTransaccion = true; // Abrir el modal\n      },\n      async guardarTransaccion(nuevaTransaccion) {\n        try {\n          const token = localStorage.getItem('token');\n          await axios.post('http://127.0.0.1:8000/api/transacciones/', nuevaTransaccion, {\n            headers: { Authorization: `Bearer ${token}` },\n          });\n          await this.cargarTransacciones(); // Recargar la lista de transacciones\n          this.mostrarModalTransaccion = false; // Cerrar el modal\n        } catch (error) {\n          console.error('Error al guardar la transacción:', error);\n          alert('Ocurrió un error al guardar la transacción.');\n        }\n      },\n      async eliminarTransaccion(id) {\n        if (confirm('¿Estás seguro de que deseas eliminar esta transacción?')) {\n          try {\n            const token = localStorage.getItem('token');\n            await axios.delete(`http://127.0.0.1:8000/api/transacciones/${id}/`, {\n              headers: { Authorization: `Bearer ${token}` },\n            });\n            await this.cargarTransacciones(); // Recargar la lista de transacciones\n          } catch (error) {\n            console.error('Error al eliminar la transacción:', error);\n            alert('Ocurrió un error al eliminar la transacción.');\n          }\n        }\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  /* Estilos personalizados si es necesario */\n  </style>"],"mappings":"AAiEE,OAAOA,MAAK,MAAO,uBAAuB;AAC1C,OAAOC,UAAS,MAAO,0BAA0B;AACjD,OAAOC,gBAAe,MAAO,oCAAoC,EAAE;AACnE,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE;IACVL,MAAM;IACNC,UAAU;IACVC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,uBAAuB,EAAE,KAAK,CAAE;IAClC,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAClC,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,mBAAmBA,CAAA,EAAG;MAC1B,IAAI;QACF,MAAME,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,0CAA0C,EAAE;UAC3EC,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUN,KAAK;UAAG;QAC9C,CAAC,CAAC;QACF,IAAI,CAACL,aAAY,GAAIQ,QAAQ,CAACT,IAAI;MACpC,EAAE,OAAOa,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;MACxD;IACF,CAAC;IACDE,WAAWA,CAACC,KAAK,EAAE;MACjB;MACA,OAAO,IAAIC,IAAI,CAACD,KAAK,CAAC,CAACE,kBAAkB,CAAC,OAAO,EAAE;QACjDC,IAAI,EAAE,SAAS;QACfC,KAAK,EAAE,MAAM;QACbC,GAAG,EAAE,SAAS;QACdC,IAAI,EAAE,SAAS;QACfC,MAAM,EAAE;MACV,CAAC,CAAC;IACJ,CAAC;IACDC,0BAA0BA,CAAA,EAAG;MAC3B,IAAI,CAACtB,uBAAsB,GAAI,IAAI,EAAE;IACvC,CAAC;IACD,MAAMuB,kBAAkBA
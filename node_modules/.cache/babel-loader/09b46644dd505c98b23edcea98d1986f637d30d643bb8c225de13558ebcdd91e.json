   <label class=\"block text-green-800 font-bold mb-2\">Contraseña</label>\n          <input\n            v-model=\"password\"\n            type=\"password\"\n            class=\"w-full px-4 py-2 border rounded-lg focus:outline-none focus:border-green-700\"\n            placeholder=\"Ingrese su contraseña\"\n          />\n        </div>\n        <button\n          type=\"submit\"\n          class=\"w-full bg-green-600 text-white py-2 rounded-lg font-bold hover:bg-green-700 transition duration-300\"\n        >\n          Ingresar\n        </button>\n      </form>\n\n      <!-- Enlace de Registro (opcional) -->\n      <div class=\"mt-4 text-center\">\n        <p class=\"text-green-800\">\n          ¿No tienes una cuenta?\n          <router-link to=\"/register\" class=\"font-bold hover:underline\">Regístrate</router-link>\n        </p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:8000/api/token/', {\n          username: this.username,\n          password: this.password,\n        });\n        localStorage.setItem('token', response.data.access);\n\n        // Redirigir al dashboard después del login\n        this.$router.push('/dashboard');\n      } catch (error) {\n        console.error('Error al iniciar sesión:', error);\n        alert('Usuario o contraseña incorrectos');\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Estilos personalizados si es necesario */\n</style>"],"mappings":";AAuDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMN,KAAK,CAACO,IAAI,CAAC,kCAAkC,EAAE;UACpEL,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACFK,YAAY,